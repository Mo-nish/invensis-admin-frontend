{
  "name": "admin-portal",
  "version": "1.0.0",
  "description": "Admin Portal for Invensis Hiring Portal",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd admin-server && npm run dev",
    "client": "cd admin-client && npm start",
    "build": "cd admin-client && npm run build",
    "install-all": "npm install && cd admin-server && npm install && cd ../admin-client && npm install",
    "start": "concurrently \"npm run server\" \"npm run client\""
  },
  "keywords": [
    "admin",
    "portal",
    "hr",
    "management"
  ],
  "author": "Invensis Company",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
